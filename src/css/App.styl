.App {
  text-align: center;
}

.App-logo {
  animation: App-logo-spin infinite 20s linear;
  height: 80px;
}

.App-header {
  background-color: #222;
  height: 150px;
  padding: 20px;
  color: white;
}

.App-intro {
  font-size: large;
}

@keyframes App-logo-spin {
  from { transform: rotate(0deg); }
  to { transform: rotate(360deg); }
}


#face {
    fill : #FFF;
    stroke-width: 2px;
    stroke: #000;
}
#hour, #min, #sec {
    fill: #333;
}
#sec {
    stroke: #f55;
}